        "outputPath": "dist/libs/features/nx-svelte-svg-loaders",
        "main": "libs/features/nx-svelte-svg-loaders/src/index.ts",
        "tsConfig": "libs/features/nx-svelte-svg-loaders/tsconfig.lib.json",
        "assets": ["libs/features/nx-svelte-svg-loaders/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
