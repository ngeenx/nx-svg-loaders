{
  "name": "nx-react-svg-loaders",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/features/nx-react-svg-loaders/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project nx-react-svg-loaders --web",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/features/nx-react-svg-loaders"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "tailwind-build": {
      "command": "npx tailwindcss -i ./libs/utils/nx-svg-shared-utils/src/styles/base-loader.component.scss -o ./dist/libs/features/nx-react-svg-loaders/styles.css"
    }
  }
}
